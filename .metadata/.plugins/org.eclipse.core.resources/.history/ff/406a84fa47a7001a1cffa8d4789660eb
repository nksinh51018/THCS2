package com.nk.hoadonService.dto;

public class ChiTietSanPhamTheoDoanhThuDTO {
	
	private ChiTietSanPhamDTO chiTietSanPhamDTO;
	private int idChiTietSanPhamDto;
	private int tongTien;
	public ChiTietSanPhamDTO getChiTietSanPhamDTO() {
		return chiTietSanPhamDTO;
	}
	public void setChiTietSanPhamDTO(ChiTietSanPhamDTO chiTietSanPhamDTO) {
		this.chiTietSanPhamDTO = chiTietSanPhamDTO;
	}
	public int getIdChiTietSanPhamDto() {
		return idChiTietSanPhamDto;
	}
	public void setIdChiTietSanPhamDto(int idChiTietSanPhamDto) {
		this.idChiTietSanPhamDto = idChiTietSanPhamDto;
	}
	public ChiTietSanPhamTheoDoanhThuDTO(ChiTietSanPhamDTO chiTietSanPhamDTO, int idChiTietSanPhamDto, int tongTien) {
		super();
		this.idChiTietSanPhamDto = idChiTietSanPhamDto;
		this.tongTien = tongTien;
	}
	public int getTongTien() {
		return tongTien;
	}
	public void setTongTien(int tongTien) {
		this.tongTien = tongTien;
	}
	
	

}
